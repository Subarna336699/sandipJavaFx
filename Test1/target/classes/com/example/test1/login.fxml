<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.test1.LoginController" prefHeight="200.0" prefWidth="300.0">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Username:" />
        <TextField fx:id="usernameField" layoutX="100.0" layoutY="10.0" />

        <Label layoutX="14.0" layoutY="44.0" text="Password:" />
        <PasswordField fx:id="passwordField" layoutX="100.0" layoutY="40.0" />

        <Button layoutX="100.0" layoutY="80.0" text="Login" onAction="#handleLogin" />
    </children>
</AnchorPane>
